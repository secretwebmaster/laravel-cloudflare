{
    "name": "secretwebmaster/laravel-cloudflare",
    "description": "Modern PHP 8+ package for seamless Cloudflare API integration in Laravel applications. Manage zones, DNS records, firewall rules, and more.",
    "keywords": [
        "laravel",
        "cloudflare",
        "dns",
        "api",
        "firewall",
        "cloudflare-api",
        "zones",
        "cloudflare-dns"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "secretwebmaster",
            "email": "earnbyshare2016@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Secretwebmaster\\LaravelCloudflare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Secretwebmaster\\LaravelCloudflare\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Secretwebmaster\\LaravelCloudflare\\CloudflareServiceProvider"
            ],
            "aliases": {
                "Cloudflare": "Secretwebmaster\\LaravelCloudflare\\Facades\\Cloudflare"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}